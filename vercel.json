{
    "builds": [{"src":"./index.js", "use": "@vercel/node"}],
    "rewrites": [{"source": "/(.*)", "destination": "/" }],
    "routes": [
        {
            "src":"/(.*)",
            "dest": "./index.js",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }

    ]      
}